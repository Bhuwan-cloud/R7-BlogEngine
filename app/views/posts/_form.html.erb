
<%= form_with(model: post) do |form| %>
  <form>
    <div class="mb-3">
      <label class="form-label">Title</label>
       <%= form.text_field(:title, size: "22") %>
    </div>
    <div class="mb-3">
      <label class="form-label">Body</label>
       <%= form.text_area(:body, size: "25x5") %>
    </div>

    <%= form.submit("Create Post", class: "btn btn-success") %>
  </form>
  <% end %>
