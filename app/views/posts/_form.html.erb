<%= form_with(model: post) do |form| %>

  <div>
    <%= form.label(:title, style: "display: block", class:"text-warning") %>
    <%= form.text_field(:title) %>
  </div>

  <div>
    <%= form.label(:body, style: "display: block",class:"text-warning") %>
    <%= form.text_area(:body) %>
  </div>

  <div>
    <%= form.submit  %>
  </div>
<% end %>
